{
  "name": "flowva",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "supabase:types": "npx supabase gen types typescript --project-id \"$VITE_SUPABASE_URL\" --schema public > src/lib/database.types.ts",
    "env:check": "node -e \"const env = process.env; const required = ['VITE_SUPABASE_URL', 'VITE_SUPABASE_ANON_KEY']; const missing = required.filter(key => !env[key]); if (missing.length > 0) { console.error('❌ Missing env vars:', missing); process.exit(1); } console.log('✅ All required env vars are set');\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
